<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="minimum-scale=1, initial-scale=1, width=device-width, shrink-to-fit=no" />
    <title>Inflation</title>

    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="/favicon-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="/favicon-194x194.png" sizes="194x194">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,500;1,400&display=swap" rel="stylesheet">

    <% if (process.env.NODE_ENV === 'production') { %>
      <link rel="stylesheet" href="/<%= manifest['src/js/index.jsx'].css %>">
    <% } %>
    
  </head>
  <body>
    <div id="js-app"><!-- React app here --></div>

    <% if (process.env.NODE_ENV==='production' ) { %>
      <script type="module" src="/<%= manifest['src/js/index.jsx'].file %>"></script>
    <% } else { %>
      <script type="module">
        /** 
         * This script is needed as we serve the index file ourselves
         * See: https://vitejs.dev/guide/backend-integration.html
        */
        import RefreshRuntime from "/@react-refresh";
        RefreshRuntime.injectIntoGlobalHook(window);
        window.$RefreshReg$ = () => { };
        window.$RefreshSig$ = () => (type) => type;
        window.__vite_plugin_react_preamble_installed__ = true;
      </script>
      <script type="module" src="/src/js/index.jsx"></script>
    <% } %>
  </body>
</html>
